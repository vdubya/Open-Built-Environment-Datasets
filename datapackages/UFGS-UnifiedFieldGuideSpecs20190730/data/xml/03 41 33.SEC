<DTA TYPE="STRING"><HL4><HL1>COMPRESSIVE STRENGTH OF CONCRETE</HL1></HL4></DTA>
         </CEL>
      </ROW>
      <ROW AUTOHEIGHT="0" HEIGHT="20.25">
         <CEL>
<DTA TYPE="STRING">Concentrically stressed sections</DTA>
         </CEL>
         <CEL>
<DTA TYPE="STRING">Not less than 3,000 psi</DTA>
         </CEL>
      </ROW>
      <ROW AUTOHEIGHT="0" HEIGHT="20.25">
         <CEL>
<DTA TYPE="STRING">Eccentrically stressed sections</DTA>
         </CEL>
         <CEL>
<DTA TYPE="STRING">Not less than 3,500 psi</DTA>
         </CEL>
      </ROW>
      <ROW AUTOHEIGHT="0" HEIGHT="34.50">
         <CEL>
<DTA TYPE="STRING">Beams or other sections in which camber must be minimized</DTA>
