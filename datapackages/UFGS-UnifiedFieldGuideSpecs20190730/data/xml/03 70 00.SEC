<DTA TYPE="STRING">Chert, less than 2.40 specific gravity</DTA>
         </CEL>
         <CEL>
<DTA TYPE="STRING">Every 6 months</DTA>
         </CEL>
         <CEL>
<DTA TYPE="STRING">Every 6 months</DTA>
         </CEL>
         <CEL>
<DTA TYPE="STRING"><RID>ASTM C123/C123M</RID></DTA>
         </CEL>
      </ROW>
      <ROW AUTOHEIGHT="0" HEIGHT="47.25">
         <CEL>
<DTA TYPE="STRING">Coal and lignite, less than 2.00 specific gravity</DTA>
         </CEL>
         <CEL>
<DTA TYPE="STRING">Every 6 months</DTA>
         </CEL>
         <CEL>
<DTA TYPE="STRING">Every 6 months</DTA>
         </CEL>
         <CEL>
<DTA TYPE="STRING"><RID>ASTM C123/C123M</RID></DTA>
         </CEL>
